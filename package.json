{
  "name": "mcp-terminal",
  "version": "1.0.46",
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.64.0",
    "@google/generative-ai": "^0.24.1",
    "@inkjs/ui": "^2.0.0",
    "@libsql/client": "^0.15.15",
    "@mendable/firecrawl-js": "^4.3.6",
    "better-sqlite3": "^12.4.1",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "external-editor": "^3.1.0",
    "ink": "^6.3.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^16.3.0",
    "marked-terminal": "^7.3.0",
    "minimist": "^1.2.8",
    "react": "^19.1.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "bin": {
    "mcp-setup": "./setup.js",
    "ask": "./mcp-assistant.js",
    "mcp-monitor": "./mcp-client.js",
    "ipcom-chat": "./ipcom-chat",
    "mcp": "./ipcom-chat"
  },
  "scripts": {
    "test": "node --test tests/*.test.js",
    "test:cancellation": "node --test tests/cancellation.test.js",
    "test:snapshots": "node --test tests/fullHistory.snapshot.test.js",
    "test:regression": "npm run test:cancellation && npm run test:snapshots",
    "test:watch": "node --test --watch tests/*.test.js",
    "test:example": "node --test tests/example.test.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx scripts/migrate.ts",
    "db:backup-and-migrate": "npx tsx scripts/backup-and-migrate.ts",
    "db:studio": "drizzle-kit studio",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/mcp-ink-cli.mjs",
    "install-script": "npm run build; node setup.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fuse.js": "^7.1.0",
    "globals": "^16.4.0",
    "ink-testing-library": "^4.0.0",
    "prettier": "3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
