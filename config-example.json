  "// Modelos disponíveis Gemini": [
    "gemini-pro",
    "gemini-pro-vision"
  ],

  "// Seleção do Provedor Principal": "",
  "provider": "anthropic",
  "// Opções de provider": ["anthropic", "openai", "gemini"],

  "// Modelo padrão (retrocompatibilidade)": "",
  "model": "claude-3-7-sonnet-20250219",

  "// Configurações de Monitoramento": "",
  "max_calls_per_hour": 100,
  "enable_monitoring": true,
  "enable_assistant": true,
  "monitor_commands": ["npm", "yarn", "git", "docker", "make", "cargo", "go"],
  "quick_fixes": true,
  "auto_detect_fixes": false,
  "log_level": "info",
  "cache_duration_hours": 24,

  "// Configuração de Web Search": "",
  "web_search": {
    "enabled": true,
    "cache_settings": {
      "documentation": 7,
      "error_solutions": 1,
      "package_info": 0.04,
      "man_pages": 30
    },
    "priority_sources": [
      "man_pages",
      "official_docs",
      "github_issues",
      "stackoverflow"
    ],
    "rate_limit_per_hour": 50
  },

  "// Configuração do Firecrawl (para scraping de sites)": "",
  "firecrawl_api_key": "fc-..."
}